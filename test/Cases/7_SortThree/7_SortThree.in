var t, a, b, c;
begin
    read(a, b, c);
    if a > b then
        begin
            t := a; a := b; b := t;
        end;
    if a > c then
        begin
            t := a; a := c; c := t;
        end;
    if b > c then
        begin
            t := b; b := c; c := t;
        end;
    write(a, b, c);
end.